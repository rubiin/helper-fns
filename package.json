{
  "name": "helper-fns",
  "type": "module",
  "version": "2.5.22",
  "packageManager": "pnpm@7.2.1",
  "description": "Some common utilities functions for everyday backend usage with zero dependencies",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubiin/helper-fns#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/helper-fns.git"
  },
  "bugs": "https://github.com/rubiin/helper-fns/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup && copyfiles -f ./src/data/*.json ./dist/data",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && clean-publish",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"tests/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@antfu/ni": "^0.18.8",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "bumpp": "^8.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.32.0",
    "jest": "29.3.1",
    "prettier": "2.8.3",
    "rimraf": "^4.0.5",
    "ts-jest": "29.0.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "clean-publish": "^4.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testTimeout": 30000,
    "rootDir": "test",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}

{
  "name": "helper-fns",
  "type": "module",
  "version": "2.6.6",
  "packageManager": "pnpm@7.2.1",
  "description": "Some common utilities functions for everyday backend usage with zero dependencies",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubiin/helper-fns#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/helper-fns.git"
  },
  "bugs": "https://github.com/rubiin/helper-fns/issues",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && clean-publish",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"tests/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "bumpp": "^9.1.1",
    "clean-publish": "^4.2.0",
    "eslint": "^8.43.0",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "29.1.0",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testTimeout": 30000,
    "rootDir": "test",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}

{
  "name": "helper-fns",
  "type": "module",
  "version": "2.5.11",
  "packageManager": "pnpm@7.2.1",
  "description": "Some common utilities functions for everyday backend usage with zero dependencies",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubiin/helper-fns#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/helper-fns.git"
  },
  "bugs": "https://github.com/rubiin/helper-fns/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@antfu/ni": "^0.17.2",
    "@types/node": "^18.7.13",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.0",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  }
}
